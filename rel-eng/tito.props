[buildconfig]
builder = tito.builder.GemBuilder
tagger = tito.tagger.VersionTagger
changelog_do_not_remove_cherrypick = 0
changelog_format = %s (%ae)

[fusor-nightly-rhel6]
disttag = .el6
blacklist =
scl = ruby193

[fusor-nightly-rhel7]
disttag = .el7
blacklist =
scl = ruby193

[requirements]
tito=0.4.12

[version_template]
template_file = ./version.rb.tmpl
destination_file = ./lib/foretello_api_v21/version.rb
